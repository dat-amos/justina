<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.justina.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <activity
        android:id="@+id/nav_gallery"
        android:name="com.example.justina.MainActivity2"
        android:label="@string/menu_gallery"
        tools:layout="@layout/activity_main2" />

    <activity
        android:id="@+id/nav_slideshow"
        android:name="com.example.justina.SlideshowActivity"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/activity_slideshow" />
    <fragment
        android:id="@+id/nav_logout"
        android:name="com.example.justina.ui.logout.LogoutFragment"
        android:label="@string/menu_logout"
        tools:layout="@layout/fragment_logout" />
    <activity
        android:id="@+id/nav_story"
        android:name="com.example.justina.StoryActivity"
        android:label="@string/menu_story"
        tools:layout="@layout/activity_story" />
    <activity
        android:id="@+id/nav_contact"
        android:name="com.example.justina.ContactActivity"
        android:label="@string/menu_contact"
        tools:layout="@layout/activity_contact" />
</navigation>